{
  "extends" : "airbnb",
  "rules" : {
    "comma-dangle" : 0,
    "no-constant-condition" : 0,
    "no-underscore-dangle" : 0,
    "no-param-reassign" : 0,
    "no-script-url" : 0,
    "no-new" : 0,
    "max-len" : [1, 200, 2],
    "react/prop-types" : 0,
    "react/jsx-filename-extension" : 0,
    "react/no-danger" : 0,
    "react/prefer-stateless-function" : 0
  },
  "globals": {
    "twttr": false,
    "document": false
  }
}
